		DaHoodTrainer = true;
		
		CombatWarriors = true;

		PhantomForces = false;
		UknownGame = false;

	};


	--// Flags_Silent
	WillDetectSilent = { -- [ "true" = Fetch_Method_Select(LoadBypass, Spoof, MouseProperties) / Detected / NaN ] --

		Dahood = true;
		DahoodVC = true;
		HoodModded = false;
		HoodDuels = true;
		TheHood = true;
		UntitledHood = true;
		HoodCustoms = true;
