--// ## !DO NOT EDIT SCRIPT MATERIAL IT WILL BREAK! ##


--// Debounce
local UpdatingWinterTime = false


--// Awaiting_Update
function Is_Updating()
	pcall(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Nosssa/NossLock/main/WinterTimeCURRENTLYUPDATING"))()
	end)
end


--// Playable_State
function _Allow()
	pcall(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Nosssa/NossLock/main/WinterTimeBOOTSTRAPPER"))()
	end)
end


--// Sort_Debounce
if UpdatingWinterTime == false then
	_Allow()
elseif UpdatingWinterTime == true then
	Is_Updating()
end
